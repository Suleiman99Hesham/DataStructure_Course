#include <iostream>
#include <bits/stdc++.h>
#include "StudentName.h"
using namespace std;

int main()
{
    string names;
    int p1=0,p2=0;
    cout<<"Please enter the name: ";
    getline(cin,names);
    StudentName F(names);
    F.print();
    cout<<"\nplease enter the names places you want to swap: ";
    cin>>p1>>p2;
    F.Replace(p1,p2);
    return 0;
}
